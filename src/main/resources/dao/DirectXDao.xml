<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.ingsoft.dao.DirectXDao">
    <select id="DirectXList" resultType="bo.ucb.edu.ingsoft.models.Directx">
        SELECT
               id_directx as idDirectx,
               version as directx
        FROM directx
        WHERE
            status = 1
    </select>
</mapper>
